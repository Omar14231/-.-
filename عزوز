	local playerTextBox = playerSection:FindFirstChild("PlayerTextBox")
	if not playerTextBox then return end
	
	if #playerName < 2 then
		playerTextBox.Text = "Ø£Ø¯Ø®Ù„ Ø­Ø±ÙÙŠÙ† Ø¹Ù„Ù‰ Ø§Ù„Ø£Ù‚Ù„"
		task.wait(1)
		playerTextBox.Text = ""
		return
	end
	
	local players = Players:GetPlayers()
	local foundPlayers = {}
	
	for _, player in pairs(players) do
		if string.find(string.lower(player.Name), string.lower(playerName)) then
			table.insert(foundPlayers, player)
		end
	end
	
	if #foundPlayers == 1 then
		-- Ø¥Ø°Ø§ ÙˆØ¬Ø¯ Ù„Ø§Ø¹Ø¨ ÙˆØ§Ø­Ø¯ ÙÙ‚Ø·ØŒ Ø§Ù†Ù‚Ù„ Ø§Ù„ÙƒØ§Ù…ÙŠØ±Ø§ Ø¥Ù„ÙŠÙ‡
		local targetPlayer = foundPlayers[1]
		if targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
			-- Ù†Ù‚Ù„ Ø§Ù„ÙƒØ§Ù…ÙŠØ±Ø§ Ø¥Ù„Ù‰ Ø§Ù„Ù„Ø§Ø¹Ø¨
			workspace.CurrentCamera.CameraSubject = targetPlayer.Character.Humanoid
			workspace.CurrentCamera.CFrame = CFrame.new(
				targetPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 10),
				targetPlayer.Character.HumanoidRootPart.Position
			)
			
			playerTextBox.Text = "ØªÙ… Ø§Ù„Ù†Ù‚Ù„ Ø¥Ù„Ù‰: " .. targetPlayer.Name
			task.wait(2)
			playerTextBox.Text = ""
		end
	elseif #foundPlayers > 1 then
		playerTextBox.Text = "ÙˆØ¬Ø¯Øª " .. #foundPlayers .. " Ù„Ø§Ø¹Ø¨ÙŠÙ†"
		task.wait(2)
		playerTextBox.Text = ""
	else
		playerTextBox.Text = "Ù„Ù… ÙŠØªÙ… Ø§Ù„Ø¹Ø«ÙˆØ± Ø¹Ù„Ù‰ Ù„Ø§Ø¹Ø¨"
		task.wait(1)
		playerTextBox.Text = ""
	end
end

-- Ø¯Ø§Ù„Ø© Ù„ØªØ´ØºÙŠÙ„ Ø§Ù„Ù…ÙˆØ³ÙŠÙ‚Ù‰
local function playMusic(soundId)
	-- Ø§Ù„Ø¨Ø­Ø« Ø¹Ù† Ø§Ù„Ø¹Ù†Ø§ØµØ±
	local musicTextBox = musicSection:FindFirstChild("MusicTextBox")
	if not musicTextBox then return end
	
	if soundId == "" then return end
	
	-- ØªÙ†Ø¸ÙŠÙ Ø§Ù„ØµÙˆØª Ø§Ù„Ù‚Ø¯ÙŠÙ…
	stopMusic()
	
	-- Ø¥Ù†Ø´Ø§Ø¡ ØµÙˆØª Ø¬Ø¯ÙŠØ¯
	local newSound = Instance.new("Sound")
	newSound.Name = "RsScriptMusic"
	newSound.SoundId = "rbxassetid://" .. soundId
	newSound.Volume = 0.5
	newSound.Looped = false
	newSound.Parent = workspace
	
	-- Ù…Ø­Ø§ÙˆÙ„Ø© ØªØ´ØºÙŠÙ„ Ø§Ù„ØµÙˆØª
	local success, errorMsg = pcall(function()
		newSound:Play()
	end)
	
	if success then
		musicTextBox.Text = "Ø¬Ø§Ø±ÙŠ Ø§Ù„ØªØ´ØºÙŠÙ„..."
		task.wait(1)
		musicTextBox.Text = soundId
	else
		musicTextBox.Text = "Ø®Ø·Ø£ ÙÙŠ ID Ø§Ù„ØµÙˆØª"
		task.wait(1)
		musicTextBox.Text = ""
	end
end

-- Ø¯Ø§Ù„Ø© Ù„Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù…ÙˆØ³ÙŠÙ‚Ù‰
local function stopMusic()
	local oldSound = workspace:FindFirstChild("RsScriptMusic")
	if oldSound then
		oldSound:Stop()
		oldSound:Destroy()
	end
end

-- Ø¯Ø§Ù„Ø© Ù„ØªØ¹Ø¯ÙŠÙ„ Ø§Ù„Ø³Ø±Ø¹Ø©
local function changeSpeed(newSpeed)
	-- Ø§Ù„Ø¨Ø­Ø« Ø¹Ù† Ø§Ù„Ø¹Ù†Ø§ØµØ±
	local speedTextBox = speedSection:FindFirstChild("SpeedTextBox")
	if not speedTextBox then return end
	
	local speedNumber = tonumber(newSpeed)
	
	if not speedNumber then
		speedTextBox.Text = "Ø£Ø¯Ø®Ù„ Ø±Ù‚Ù… ØµØ­ÙŠØ­"
		task.wait(1)
		speedTextBox.Text = ""
		return
	end
	
	if speedNumber < 0 or speedNumber > 1000 then
		speedTextBox.Text = "Ø§Ù„Ø³Ø±Ø¹Ø© Ø¨ÙŠÙ† 0 Ùˆ 1000"
		task.wait(1)
		speedTextBox.Text = ""
		return
	end
	
	local player = Players.LocalPlayer
	if player.Character then
		local humanoid = player.Character:FindFirstChild("Humanoid")
		if humanoid then
			humanoid.WalkSpeed = speedNumber
			speedTextBox.Text = "ØªÙ… Ø§Ù„ØªØ·Ø¨ÙŠÙ‚: " .. speedNumber
			task.wait(1)
			speedTextBox.Text = tostring(speedNumber)
		end
	end
end

