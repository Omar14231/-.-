-- Ø¯Ø§Ù„Ø© Ù„Ù„ØªØ¯Ø±ÙŠØ¬
local function fadeIn(object, property, targetValue, duration)
	local tweenInfo = TweenInfo.new(
		duration,
		Enum.EasingStyle.Quad,
		Enum.EasingDirection.Out
	)
	
	local tween = TweenService:Create(object, tweenInfo, {[property] = targetValue})
	tween:Play()
	return tween
end

-- Ø¯Ø§Ù„Ø© Ù„Ù„Ø¸Ù‡ÙˆØ± Ø¨Ø§Ù„ØªØ¯Ø±ÙŠØ¬
local function fadeOut(object, property, targetValue, duration)
	local tweenInfo = TweenInfo.new(
		duration,
		Enum.EasingStyle.Quad,
		Enum.EasingDirection.Out
	)
	
	local tween = TweenService:Create(object, tweenInfo, {[property] = targetValue})
	tween:Play()
	return tween
end

-- Ø¯Ø§Ù„Ø© Ù„ÙØªØ­/Ø¥ØºÙ„Ø§Ù‚ Ø§Ù„Ù‚Ø§Ø¦Ù…Ø©
local function toggleMenu(isOpening)
	if isOpening then
		-- ÙØªØ­ Ø§Ù„Ù‚Ø§Ø¦Ù…Ø©
		controlFrame.Visible = true
		closeButton.Visible = true
		
		-- ØªØ£Ø«ÙŠØ± Ø§Ù„Ø¸Ù‡ÙˆØ±
		controlFrame.Position = UDim2.new(0, 20, 0.5, -250)
		controlFrame.BackgroundTransparency = 1
		closeButton.BackgroundTransparency = 1
		
		local frameFade = fadeIn(controlFrame, "BackgroundTransparency", 0.1, 0.3)
		local closeFade = fadeIn(closeButton, "BackgroundTransparency", 0, 0.3)
		
		-- ØªØ­Ø±ÙŠÙƒ Ù…Ù† Ø§Ù„ÙŠØ³Ø§Ø±
		local slideIn = TweenService:Create(
			controlFrame,
			TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
			{Position = UDim2.new(0, 20, 0.5, -250)}
		)
		slideIn:Play()
		
	else
		-- Ø¥ØºÙ„Ø§Ù‚ Ø§Ù„Ù‚Ø§Ø¦Ù…Ø©
		local frameFade = fadeOut(controlFrame, "BackgroundTransparency", 1, 0.3)
		local closeFade = fadeOut(closeButton, "BackgroundTransparency", 1, 0.3)
		
		-- ØªØ­Ø±ÙŠÙƒ Ø¥Ù„Ù‰ Ø§Ù„ÙŠØ³Ø§Ø±
		local slideOut = TweenService:Create(
			controlFrame,
			TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In),
			{Position = UDim2.new(-0.5, 0, 0.5, -250)}
		)
		slideOut:Play()
		
		task.wait(0.4)
		controlFrame.Visible = false
		closeButton.Visible = false
	end
end

